<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.runjian.auth.server.mapper.role.SysRoleMenuMapper">
    <select id="selectMenuByRoleId" resultType="java.lang.Long">
        SELECT menu_id
        FROM tb_sys_role_menu rm
        WHERE rm.role_id = #{roleId,jdbcType=BIGINT}
    </select>
</mapper>
